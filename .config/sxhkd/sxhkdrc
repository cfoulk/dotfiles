alt + x
    dmenuunicode

alt + w
    firefox

XF86AudioRaiseVolume
    wpctl set-volume @DEFAULT_AUDIO_SINK@ 2%+; kill -49 $(pidof dwmblocks)

XF86AudioLowerVolume
    wpctl set-volume @DEFAULT_AUDIO_SINK@ 2%-; kill -49 $(pidof dwmblocks)

XF86AudioMute
    wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle; kill -49 $(pidof dwmblocks)

